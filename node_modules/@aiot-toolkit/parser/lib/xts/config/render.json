[
  {
    "name": "this.",
    "rule": [
      {
        "decorator": "@Renderer"
      }
    ]
  },
  {
    "name": "ForEach",
    "rule": [
      {
        "paramIndex": 0,
        "type": "PropertyAccessExpression",
        "desc": "必须是数组，允许设置为空数组，空数组场景下将不会创建子组件。"
      },
      {
        "paramIndex": 1,
        "type": "ArrowFunction",
        "desc": "生成子组件的lambda函数，为数组中的每一个数据项创建一个或多个子组件，单个子组件或子组件列表必须包括在大括号“{...}”中。"
      },
      {
        "paramIndex": 2,
        "type": "ArrowFunction",
        "isRequired": false,
        "desc": "匿名函数，用于给数组中的每一个数据项生成唯一且固定的键值。"
      }
    ]
  }
]
