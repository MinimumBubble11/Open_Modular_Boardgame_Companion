{
  "name": "eslint-plugin-ux",
  "version": "0.0.4",
  "description": "Official ESLint plugin for ux",
  "main": "lib/index.js",
  "scripts": {
    "debug": "mocha --inspect-brk \"tests/lib/**/*.js\" --reporter dot --timeout 60000",
    "start": "npm run test:base -- --watch --growl",
    "test": "nyc npm run test:base -- \"tests/integrations/*.js\" --timeout 60000",
    "test:base": "mocha \"tests/lib/**/*.js\" --reporter dot"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-config",
    "ux",
    "vue",
    "vuejs",
    "rules"
  ],
  "engines": {
    "node": ">=8.10"
  },
  "peerDependencies": {
    "eslint": "^5.0.0 || ^6.0.0"
  },
  "dependencies": {
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "@types/node": "^4.2.16",
    "@typescript-eslint/parser": "^2.6.1",
    "acorn": "^7.1.0",
    "babel-eslint": "^10.0.2",
    "chai": "^4.1.0",
    "eslint": "^6.0.0",
    "eslint-plugin-eslint-plugin": "^2.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-ux": "file:.",
    "eslint-plugin-vue-libs": "^4.0.0",
    "eslint-visitor-keys": "^1.1.0",
    "eslint4b": "^6.6.0",
    "lodash": "^4.17.4",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "typescript": "^3.5.2",
    "vue-eslint-editor": "^0.1.4"
  }
}
